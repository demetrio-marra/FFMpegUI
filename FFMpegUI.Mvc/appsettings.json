{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RavenDb": {
    "Urls": [ "your-ravendb-url" ],
    "Database": "your-database-name",
    "CertFilePath": "your-certificate-file-path", // if applicable
    "CertPassword": "your-certificate-password" // if applicable
  },
  "FFMpegUI": {
    "SourceFilesDirectoryPath": "/var/ffmpegui/sourcefiles",
    "ConvertedFilesDirectoryPath": "/var/ffmpegui/convertedfiles"
  }
}
